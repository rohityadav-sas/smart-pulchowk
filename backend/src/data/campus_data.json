  "frequently_asked_questions": {
    "lost_id_card": {
      "question": "Where do I go if I lost my ID card?",
      "answer": "Go to the Dean Office on the Ground Floor for ID card replacement. You will need to fill out a form and pay a replacement fee.",
      "location_id": "dean-office"
    },
    "toilets": {
      "question": "Where are the toilets?",
      "answer": "The main public toilets are: 1) Ground floor of Pulchowk Library, 2) Third floor of Department of Electrical, Electronics and Computer Engineering, 3) Near the first entrance (standalone toilet), 4) Changing room and toilet near sports grounds.",
      "location_ids": ["pulchowk-library", "dept-electrical-electronics-computer", "toilet-1", "changing-room-toilet"]
    },
    "drinking_water": {
      "question": "Where can I get drinking water?",
      "answer": "There is a Water Vending Machine near the library (Rs. 1 per liter for purified water), and public water taps near the second entrance.",
      "location_ids": ["water-vending-machine", "water-tap"]
    },
    "atm": {
      "question": "Where is the ATM?",
      "answer": "There are two ATMs on campus: Nabil Bank ATM (near Saheed Shukra Park) and Siddhartha Bank ATM (near the first entrance).",
      "location_ids": ["nabil-bank-atm", "siddhartha-bank-atm"]
    },
    "printing": {
      "question": "Where can I print documents?",
      "answer": "Om Stationery Store near the second entrance offers printing, photocopying, and binding services.",
      "location_id": "om-stationery"
    },
    "food": {
      "question": "Where can I eat on campus?",
      "answer": "Campus Mess (for hostel students), Campus Canteen near the sports grounds, and Niraula Stores for snacks.",
      "location_ids": ["campus-mess", "campus-canteen", "niraula-stores"]
    },
    "library_hours": {
      "question": "What are the library hours?",
      "answer": "The library is open from 9:30 AM to 5:30 PM during summer hours. All students, faculty, and staff are eligible for membership.",
      "location_id": "pulchowk-library"
    },
    "gym": {
      "question": "Where is the gym?",
      "answer": "Pulchowk Gym Hall is located near the hostel area and is open from 9 AM to 6 PM. There is also an outdoor calisthenics park.",
      "location_ids": ["gym-hall", "outdoor-gym"]
    },
    "hostel": {
      "question": "Where are the hostels?",
      "answer": "Boys hostels are in Block A, B, and C near the sports grounds. Girls hostel is located further north with its own canteen. MSC Hostel is for Masters students.",
      "location_ids": ["block-a-hostel", "block-b-hostel", "block-c-hostel", "girls-hostel", "msc-hostel"]
    },
